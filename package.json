{
  "name": "gulp-browserify-kit",
  "version": "1.0.0",
  "description": "sample kit for using gulp + browserify",
  "author": "Chris Wigley <cwigley@onenorth.com>",
  "keywords": [],
  "//": [],
  "license": {
    "type": "MIT",
    "url": "http://onenorth.mit-license.org"
  },
  "private": true,
  "scripts": {
    "build": "./node_modules/.bin/gulp build:production",
    "build-dev": "./node_modules/.bin/gulp build",
    "dev": "./node_modules/.bin/gulp watch",
    "test": "./node_modules/.bin/mocha --reporter spec",
    "postinstall": "./node_modules/.bin/gulp default"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/onenorth/gulp-browserify-kit.git"
  },
  "bugs": {
    "url": "https://github.com/onenorth/gulp-browserify-kit/issues"
  },
  "browser": {
    "underscore": "backbone/node_modules/underscore",
    "jquery": "./node_modules/jquery/dist/jquery.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "jquery-plugin": {
      "exports": "jquery-plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "^1.9.1",
    "browserify": "^8.1.1",
    "browserify-shim": "^3.8.2",
    "del": "^1.1.1",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-base64": "^0.1.2",
    "gulp-cached": "^1.0.2",
    "gulp-changed": "^1.1.0",
    "gulp-csslint": "^0.1.5",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.1.0",
    "gulp-jshint": "^1.9.0",
    "gulp-load-plugins": "^0.8.0",
    "gulp-minify-css": "^0.4.2",
    "gulp-notify": "^2.1.0",
    "gulp-plumber": "^0.6.6",
    "gulp-sass": "^1.3.2",
    "gulp-size": "^1.2.0",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-uglify": "^1.1.0",
    "gulp-uncss": "^1.0.0",
    "gulp-util": "^3.0.2",
    "handlebars": "^2.0.0",
    "hbsfy": "^2.2.1",
    "jshint-stylish": "^1.0.0",
    "lodash": "^3.0.0",
    "minimist": "^1.1.0",
    "mocha": "^2.1.0",
    "pretty-hrtime": "^1.0.0",
    "run-sequence": "^1.0.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.2.1",
    "yargs": "^1.3.3"
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "jquery": "^2.1.3"
  }
}
